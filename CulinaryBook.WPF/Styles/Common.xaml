<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--COLORS-->
    <Color x:Key="PrimaryColor1">#F67B50</Color>
    <Color x:Key="PrimaryColor2">#FBAA60</Color>
    <Color x:Key="SecondaryColor1">#FBC490</Color>
    <Color x:Key="SecondaryColor2">Gray</Color>
    <Color x:Key="TextColor1">#000</Color>
    <Color x:Key="TextColor2">#A82810</Color>
    <!--BRUSHES-->
    <SolidColorBrush x:Key="PrimaryBrush1" Color="{StaticResource PrimaryColor1}" />
    <SolidColorBrush x:Key="PrimaryBrush2" Color="{StaticResource PrimaryColor2}" />
    <SolidColorBrush x:Key="SecondaryBrush1" Color="{StaticResource SecondaryColor1}" />
    <SolidColorBrush x:Key="SecondaryBrush2" Color="{StaticResource TextColor1}" />
    <SolidColorBrush x:Key="SecondaryBrush3" Color="{StaticResource TextColor2}" />
    <SolidColorBrush x:Key="SecondaryBrush4" Color="{StaticResource SecondaryColor2}" />
</ResourceDictionary>